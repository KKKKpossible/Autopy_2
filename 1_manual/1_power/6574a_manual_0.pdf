<script>
	function resizeCTAButtons() {
					function fixButtonSize(buttons) {
									var maxWidth = 0;
									buttons.each(function() {
													var w = this.offsetWidth;
													this.style.width = 'auto';
													maxWidth = Math.max(maxWidth, this.offsetWidth);
													this.style.width = (w + 1) + 'px';
									});
									buttons.each(function() {
													this.style.width = (maxWidth + 1) + 'px';
													this.className += ' resized';
									});
					}
					function resizeButtons() {
									var $this = $(this);
									$this.find('.btn').removeClass('resized');
									$this.find('.layout-multicolumn .layout-multicolumn').each(resizeButtons);
									fixButtonSize($this.find('.left-wide > div:first-child .btn:not(.resized)'));
									fixButtonSize($this.find('.left-wide > div + div .btn:not(.resized)'));
									fixButtonSize($this.find('.right-wide > div:first-child .btn:not(.resized)'));
									fixButtonSize($this.find('.right-wide > div + div .btn:not(.resized)'));
									fixButtonSize($this.find('.two-columns .two-columns .btn:not(.resized)'));
									fixButtonSize($this.find('.btn:not(.resized)'));
									$this.addClass('.btn-resized');
					}
					$('#mainsection .wrapper:not(.btn-resized)').each(resizeButtons);
	}
</script>

